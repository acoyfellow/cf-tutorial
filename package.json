{
  "name": "cf-tutorial",
  "version": "0.0.1",
  "description": "Interactive Cloudflare learning platform with AI-powered quiz evaluation",
  "author": "acoyfellow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acoyfellow/cf-tutorial"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "sveltekit",
    "quiz",
    "learning",
    "workers-ai"
  ],
  "scripts": {
    "dev": "alchemy dev",
    "deploy": "alchemy deploy",
    "destroy": "alchemy destroy",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:better-auth": "bunx @better-auth/cli generate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:local": "wrangler d1 migrations apply cf-tutorial-db --local",
    "db:remote": "wrangler d1 migrations apply cf-tutorial-db --remote"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.561.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.8",
    "svelte": "^5.46.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "@cf-wasm/resvg": "^0.3.3",
    "@types/node": "^25.0.2",
    "alchemy": "^0.81.3",
    "better-auth": "1.4.7",
    "drizzle-orm": "^0.45.1",
    "marked": "^17.0.1",
    "svelte-splitpanes": "^8.0.12"
  },
  "type": "module"
}
